<?php
	
	// Valid logging methods:
	// - Debug
	// - Info
	// - Error
	
	class Log {
		// Logging "types"
		const Debug = 'Debug', Info = 'Info', Error = 'Error';
		
		public static function write($type, $string) {
			// For now, just print everything
			print "[{$type}] {$string}\r\n";
		}
	};






?>
